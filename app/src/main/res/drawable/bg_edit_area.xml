<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            android:shape="line">
            <stroke android:width="1dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp"/>
            <corners android:radius="12dp"/>
            <solid android:color="#fff"/>
            <padding
                android:left="8dp"
                android:top="12dp"
                android:bottom="12dp"
                android:right="8dp"/>

        </shape>
    </item>
</layer-list>